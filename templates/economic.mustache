<div class="reportSection tableContainer">
  <h4>Overlap with Fisheries Closures</h4>
  <table data-paging="10" data-no-rows="Does not overlap any Fisheries Closures">
    <thead>
      <tr>
        <th>Tenures</th>
        <th>Overlap (km²)</th>
        <th>Overlap %</th>
      </tr>
    </thead>
    <tbody>
      {{#closures}}
      <tr>
        <td>{{NAME}}</td>
        <td>{{CLPD_AREA}}</td>
        <td>{{PERC_AREA}}</td>
      </tr>
      {{/closures}}
    </tbody>
  </table>
  <!-- <a href="#" data-toggle-node="51f5545c08dc4f5f2d216146" data-visible="false">show habitats layer</a> -->
</div>

